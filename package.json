{
  "name": "genrandom",
  "description": "genrandom (generate random) is a (numeric or alpha numeric) generator by length and segments",
  "main": "dist/genrandom.js",
  "version": "0.0.0-development",
  "scripts": {
    "commit": "git-cz",
    "check-coverage": "nyc check-coverage --statements 20 --branches 20 --functions 20 --lines 20",
    "report-coverage": "nyc report --reporter=text-lcov > coverage.lcov",
    "watch:test": "npm t -- -w",
    "prebuild": "rimraf dist",
    "build": "npm-run-all --parallel build:*",
    "build:main": "babel --copy-files --out-dir dist --ignore *.test.js src --no-comments",
    "build:umd": "webpack --output-filename genrandom.umd.js",
    "build:umd.min": "webpack --output-filename genrandom.umd.min.js -p",
    "test": "mocha src/genrandom.test.js --require babel-core/register",
    "start": "npm run test",
    "cover": "nyc npm t",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jwright04/genrandom.git"
  },
  "keywords": [
    "random",
    "numeric",
    "alpha",
    "number",
    "num",
    "generator",
    "randomly",
    "randomizer"
  ],
  "files": [
    "dist",
    "umd",
    "README.md"
  ],
  "author": "James C. Wright <me@jamescwright.com> (http://jamescwright.com/)",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@semantic-release/changelog": "3.0.4",
    "@semantic-release/git": "7.0.16",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-loader": "8.0.6",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "babel-register": "6.26.0",
    "chai": "4.2.0",
    "codecov": "3.5.0",
    "codecov.io": "0.1.6",
    "commitizen": "3.1.1",
    "cz-conventional-changelog": "2.1.0",
    "ghooks": "2.0.4",
    "istanbul": "0.4.5",
    "json-loader": "0.5.7",
    "mocha": "6.1.4",
    "npm-run-all": "4.1.5",
    "nyc": "14.1.1",
    "rimraf": "2.6.3",
    "semantic-release": "^15.13.18",
    "webpack": "4.35.2",
    "webpack-cli": "3.3.5"
  },
  "engines": {
    "node": "10.16.x",
    "npm": "6.9.x"
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run cover && npm run check-coverage"
    }
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  },
  "release": {
    "prepare": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      {
        "path": "@semantic-release/git",
        "assets": [
          "package.json",
          "package-lock.json",
          "CHANGELOG.md"
        ],
        "message": "chore(release): ${nextRelease.version} [skip ci]nn${nextRelease.notes}"
      }
    ]
  }
}
